<%- include("index") %>
    <h2>Products Table</h2>
    <div>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                    <th>Price</th>
                    <th>Rating</th>
                    <th>Description</th>
                    <th>Created At</th>
                </tr>
            </thead>
            <tbody>
                <% allProducts.map(product=> { %>
                    <tr>
                        <td>
                            <%= product._id %>
                        </td>
                        <td>
                            <%= product.title %>
                        </td>
                        <td>
                            <%= product.price %>
                        </td>
                        <td>
                            <%= product.rating %>
                        </td>
                        <td>
                            <%= product.desc %>
                        </td>
                        <td>
                            <%= product.createdAt %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <%- include("footer") %>